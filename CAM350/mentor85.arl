NAME  Mentor_85
# ***********************************
# This rule file works with Mentor ver 8.5 in English Format tables.
FILE_EXTENSION txt

VERSION 1
UNIT INCH

# Shape definitions:
S_ROUND        circle
S_RECTANGLE    rectangle

# Line formats:
FMT_SKIPUNTIL  +0 Position $skipe
FMT_ROUND      $apid  $shape  $skip  $skip   $xsize  $skip  $skip  $skip  $dcode  $skipe
FMT_RECTANGLE  $apid  $shape  $skip  $ysize  $xsize  $skip  $skip  $skip  $dcode  $skipe
FMT_SKIPEOF    Object Painting